{
  "singleQuote": false,
  "semi": true,
  "printWidth": 100,
  "trailingComma": "all",
  "importOrder": [
    "^react",
    "^react-native",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}